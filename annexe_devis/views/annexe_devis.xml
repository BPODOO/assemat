<odoo>
  <data>
    <record model="ir.ui.view" id="annexe_devis.list">
      <field name="name">BP Annexe devis list</field>
      <field name="model">annexe.devis</field>
      <field name="arch" type="xml">
        <tree editable="top">
          <field name="name" invisible="1" force_save="1"/>
          <field name="bp_file" filename="name"/>
        </tree>
      </field>
    </record>
      
    <record id="action_annexe_devis" model="ir.actions.act_window">
        <field name="name">Annexe devis</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">annexe.devis</field>
        <field name="view_mode">list</field>
    </record>
      
    <menuitem id="annexe_devis_menu" 
    action="action_annexe_devis"
    parent="sale.menu_sale_config"
    name="Annexe devis"
    sequence="1"/>
      
  </data>
</odoo>