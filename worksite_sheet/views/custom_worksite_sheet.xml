<odoo>
  <data>
    <record id="bp_custom_worksite_sheet" model="ir.ui.view">
        <field name="name">BP - Custom Feuille Atelier</field>
        <field name="model">custom.worksite.sheet</field>
        <field name="arch" type="xml">
            <tree create="1" delete="1" editable="bottom">
                <field name="name"/>
                <field name="bp_color" widget="color"/>
            </tree>
        </field>
    </record>
      
    <record id="action_custom_worksite_sheet" model="ir.actions.act_window">
            <field name="name">Custom Feuille Atelier</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">custom.worksite.sheet</field>
            <field name="view_mode">list</field>
    </record>

    <!-- Action Menu Item Related To Above Action Window -->
    <menuitem id="action_menu_custom_worksite_sheet" 
        action="action_custom_worksite_sheet" 
        parent="project.menu_project_config"
        name="Custom Feuille Atelier"
        sequence="99"/>  

  </data>
</odoo>